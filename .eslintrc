{
  "env": {
    "amd": true,
    "browser": true,
    "es6": true,
    "jquery": true,
    "node": true
  },
  "globals": {
    "$filesTableWrapper": false,
    "AnythingDropdown": false,
    "BambooHR": false,
    "CSRF_TOKEN": false,
    "GLOBAL_COMPANY_COUNTRY_ID": false,
    "GLOBAL_DATEPICKER_MASK": false,
    "HSHQ_DATEPICKER_OPTIONS": false,
    "SimpleModal": false,
    "activeIndicator": false,
    "ajaxModalDialog": false,
    "angular": false,
    "appPrefix": false,
    "attachCSRFInput": false,
    "attachModalEvents": false,
    "bhrChosenOptions": false,
    "closeMessage": false,
    "closeModalDialog": false,
    "config": false,
    "disabledForPermsTest": false,
    "doneLoading": false,
    "empty": false,
    "errorFallBack": false,
    "fileScanTimeout": true,
    "getFileId": false,
    "loadSectionFiles": false,
    "microTemplate": false,
    "modalDialogAlert": false,
    "modalFormSubmit": false,
    "moment": false,
    "offset": false,
    "qsWithPhoto": false,
    "sessionUserId": false,
    "setMessage": false,
    "showModalPopup": false,
    "showStackedModalDialog": false,
    "validate": false,
    "validatePtoRequest": false,
    "webViewerDestroy": false,
    "webViewerLoad": false,
    "xmlEntities": false,
    // Testing Helpers
    "describe": false,
    "it": false,
    "beforeEach": false,
    "afterEach": false,
    "before": false,
    "after": false,
    "setupMockDOM": false,
    "teardownMockDOM": false
  },
  "parser": "babel-eslint",
  "rules": {
    "array-bracket-spacing": 2,
    "arrow-spacing": 2,
    "block-spacing": 2,
    "brace-style": ["error", "1tbs", { "allowSingleLine": true }],
    "comma-style": 2,
    "computed-property-spacing": 2,
    "constructor-super": 2,
    "curly": 2,
    "dot-location": [2, "property"],
    "func-call-spacing": 2,
    "func-name-matching": 2,
    "generator-star-spacing": 2,
    "global-require": 2,
    "handle-callback-err": 2,
    "id-blacklist": 2,
    "id-match": 2,
    "linebreak-style": 2,
    "lines-around-directive": 2,
    "max-depth": 2,
    "max-nested-callbacks": 2,
    "new-parens": 2,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-catch-shadow": 2,
    "no-class-assign": 2,
    "no-cond-assign": 2,
    "no-confusing-arrow": 2,
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-continue": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-label": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-iterator": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": ["error", {"skipComments": true}],
    "no-label-var": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-mixed-requires": 2,
    "no-multiple-empty-lines": 2,
    "no-multi-str": 2,
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-symbol": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-path-concat": 2,
    "no-process-env": 2,
    "no-process-exit": 2,
    "no-proto": 2,
    "no-restricted-imports": 2,
    "no-restricted-modules": 2,
    "no-restricted-syntax": 2,
    "no-return-assign": 2,
    "no-regex-spaces": 2,
    "no-restricted-globals": 2,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-sequences": 2,
    "no-shadow-restricted-names": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-undef": 1,
    "no-undef-init": 2,
    "no-unmodified-loop-condition": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unused-expressions": 2,
    "no-unused-labels": 2,
    "no-useless-computed-key": 2,
    "no-useless-rename": 2,
    "no-useless-return": 2,
    "no-void": 2,
    "no-with": 2,
    "nonblock-statement-body-position": 2,
    "operator-assignment": 2,
    "require-await": 2,
    "require-yield": 2,
    "rest-spread-spacing": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "strict": 2,
    "symbol-description": 2,
    "template-curly-spacing": 2,
    "template-tag-spacing": 2,
    "unicode-bom": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "yield-star-spacing": 2
  }
}
